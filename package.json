{
	"name": "example-with-tailwindcss",
	"type": "module",
	"scripts": {
		"dev": "bun run ./scripts/dev.ts",
		"db:up": "docker compose up -d",
		"db:down": "docker compose down",
		"zero:dev": "set -a; [ -f .env ] && . ./.env; set +a; zero-cache-dev --upstream-db \"$ZERO_UPSTREAM_DB\" --query-url \"$ZERO_QUERY_URL\" --mutate-url \"$ZERO_MUTATE_URL\" --query-forward-cookies true --mutate-forward-cookies true --log-level info",
		"build": "bun run ./scripts/build.ts",
		"start": "vinxi start",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"typecheck": "tsc --noEmit",
		"e2e": "playwright test",
		"e2e:ui": "playwright test --ui",
		"ralph-prd": "bun run ./scripts/ralph.ts --prompt-file ./docs/ralph.md"
	},
	"dependencies": {
		"@effect/platform": "^0.94.4",
		"@effect/platform-bun": "^0.87.1",
		"@effect/platform-node": "^0.104.1",
		"@rocicorp/zero": "^0.25.12",
		"@solidjs/router": "^0.15.4",
		"@solidjs/start": "^1.2.1",
		"arkregex": "^0.0.5",
		"effect": "^3.19.16",
		"postgres": "^3.4.8",
		"solid-js": "^1.9.11",
		"vinxi": "^0.5.11",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.14",
		"@effect/cli": "^0.73.2",
		"@effect/language-service": "^0.73.1",
		"@playwright/test": "^1.58.2",
		"@tailwindcss/vite": "^4.1.18",
		"@types/bun": "^1.3.9",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"vite": "^7.3.1"
	},
	"trustedDependencies": [
		"@rocicorp/zero-sqlite3"
	]
}
